{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "import/no-anonymous-default-export": ["off"],
    "no-unused-vars": ["off"]
  },
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.ts", "*.tsx"],
      "rules": {
        "no-unused-vars": ["warn", {"vars": "all", "args": "after-used", "ignoreRestSiblings": false}]
      }
    }
  ]
}
